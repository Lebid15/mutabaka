{
  "cli": {
    "version": ">= 16.19.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "withoutCredentials": false
      },
      "env": {
        "EXPO_PUBLIC_APP_ENV": "development",
        "EXPO_PUBLIC_API_BASE_URL": "https://dealing-upgrades-readily-diesel.trycloudflare.com/api",
        "EXPO_PUBLIC_WS_BASE_URL": "wss://dealing-upgrades-readily-diesel.trycloudflare.com/ws",
        "EXPO_PUBLIC_TENANT_HOST": "dealing-upgrades-readily-diesel.trycloudflare.com"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "android": {
        "buildType": "app-bundle",
        "withoutCredentials": false
      },
      "env": {
        "EXPO_PUBLIC_APP_ENV": "production",
        "EXPO_PUBLIC_API_BASE_URL": "https://mutabaka.com/api",
        "EXPO_PUBLIC_WS_BASE_URL": "wss://mutabaka.com/ws",
        "EXPO_PUBLIC_TENANT_HOST": "mutabaka.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
